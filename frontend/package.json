    "@mantine/modals": "^7.5.1",
    "@mantine/notifications": "^7.5.1",
    "@sentry/react": "^7.101.0",
    "@tanstack/react-query": "^5.20.5",
    "@tanstack/react-query-devtools": "^5.20.5",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "^18.2.0",
    "wretch": "^2.8.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/react": "^18.2.55",
